EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/Alamofire.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Mattress.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "Alamofire" -framework "Mattress"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-GrioBook
PODS_ROOT = ${SRCROOT}/Pods
SWIFT_INCLUDE_PATHS[sdk=iphoneos*] = $(SRCROOT)/Mattress/CommonCrypto/iphoneos
SWIFT_INCLUDE_PATHS[sdk=iphonesimulator*] = $(SRCROOT)/Mattress/CommonCrypto/iphonesimulator
SWIFT_INCLUDE_PATHS[sdk=macosx*] = $(SRCROOT)/Mattress/CommonCrypto/macosx